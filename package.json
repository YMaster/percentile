{
  "name": "@iusername/percentile",
  "version": "1.0.1",
  "description": "Calculate a percentile for given array of values",
  "main": "dist/percentile.js",
  "module": "es/percentile.js",
  "types": "es",
  "private": false,
  "repository": "https://github.com/YMaster/percentile.git",
  "scripts": {
    "test": "ava",
    "prebuild": "rimraf lib & rimraf es & rimraf commonjs & rimraf amd & rimraf system",
    "build": "rollup -c",
    "prepublish": "npm run build & npm run test",
    "release": "standard-version",
    "release:first": "standard-version --first-release",
    "release:alpha": "standard-version --prerelease alpha",
    "release:minor": "standard-version  --release-as minor",
    "release:noverify": "standard-version --no-verify",
    "release:publish": "git push --follow-tags origin master && npm publish",
    "cz": "git-cz"
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "eslint --fix",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "> 5%"
  ],
  "keywords": [
    "math",
    "calculate",
    "percentile",
    "PERCENTILE",
    "PERCENTILE.EXC",
    "PERCENTILE.INC"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "author": "YM",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.10.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/parser": "^7.10.5",
    "@babel/plugin-external-helpers": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/types": "^7.10.5",
    "@commitlint/config-angular": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "ava": "^3.10.1",
    "commitizen": "^4.1.2",
    "commitlint": "^9.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.4.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.22.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "standard-version": "^8.0.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7"
  }
}
