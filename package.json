{
  "name": "@iusername/percentile",
  "version": "1.0.0",
  "description": "Calculate a percentile for given array of values",
  "main": "dist/percentile.js",
  "module": "es/percentile.js",
  "types": "es",
  "repository": "https://github.com/YMaster/percentile.git",
  "scripts": {
    "test": "ava",
    "prebuild": "rimraf es && rimraf dist",
    "build": "tsc --outDir es && rollup -c",
    "postbuild": "npm run test",
    "prepublish": "npm run build",
    "cz": "git-cz"
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "eslint --fix",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "> 5%"
  ],
  "keywords": [
    "math",
    "calculate",
    "percentile",
    "PERCENTILE",
    "PERCENTILE.EXC",
    "PERCENTILE.INC"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "author": "YM",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/parser": "^7.10.1",
    "@babel/plugin-external-helpers": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/types": "^7.10.1",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "ava": "^3.8.2",
    "commitizen": "^4.1.2",
    "commitlint": "^8.3.5",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.11.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  }
}
